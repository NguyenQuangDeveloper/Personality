<Window x:Class="ChamberDemo2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChamberDemo2"
        xmlns:ctrls="clr-namespace:ChamberControl.Views;assembly=ChamberControl"
        mc:Ignorable="d"
        
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ctrls:Chamber Background="AliceBlue" ViewModel="{Binding ChamberViewModel, UpdateSourceTrigger=PropertyChanged}" />
    </Grid>
</Window>
