<UserControl x:Class="VSP_88D_CS.Views.Report.ReportPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VSP_88D_CS.Views.Report"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             xmlns:vsmvvm="clr-namespace:VSLibrary.Common.MVVM.Behaviors;assembly=VSLibrary"
             xmlns:VSControls="clr-namespace:VSLibrary.UIComponent.VSControls;assembly=VSLibrary"
             xmlns:VsNavigations="clr-namespace:VSLibrary.UIComponent.VsNavigations;assembly=VSLibrary">
    <Grid>
        <Grid.ColumnDefinitions>
            <!--<ColumnDefinition Width="85*"></ColumnDefinition>
            <ColumnDefinition Width="15*"></ColumnDefinition>-->
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="0.12*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <!--<ContentControl Grid.Column="0"
                        vsmvvm:RegionBehavior.RegionName="ReportPage"></ContentControl>
        <ContentControl Grid.Column="1"
                        vsmvvm:RegionBehavior.RegionName="ReportMenu"></ContentControl>-->
        <VSControls:VsControl Grid.Column="0"
                              ContentList="{Binding ContentList}"
                              CurrentView="{Binding CurrentView}"
                              ContentTypeList="{Binding ContentList}"/>

        <VsNavigations:VsNavigationBar Grid.Column="1"
                                       ButtonDatas="{Binding ButtonDatas}"
                                      
                                       Position="Right" />
    </Grid>
</UserControl>
