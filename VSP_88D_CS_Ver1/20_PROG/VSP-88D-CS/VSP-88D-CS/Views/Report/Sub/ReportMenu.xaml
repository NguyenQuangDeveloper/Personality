<UserControl x:Class="VSP_88D_CS.Views.Report.Sub.ReportMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:VSP_88D_CS.Views.Report.Sub"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Style="{StaticResource ImageButton2Style}"
                    Content="{Binding LanguageResources[Log], FallbackValue='Log'}"
                    Tag="{Binding BtnLogInfo}"
                    Command="{Binding ReportMenuBtnCommand }"
                    CommandParameter="{Binding RelativeSource={RelativeSource Self}}">
                <!--IsEnabled="{Binding IsSelected}">-->
                    
            </Button>



            <Button Style="{StaticResource ImageButton2Style}"
                    Content="{Binding LanguageResources[Chart], FallbackValue='Chart'}"
                    Background="White"
                    Tag="{Binding BtnChartInfo}"
                    Command="{Binding ReportMenuBtnCommand}"
                    CommandParameter="{Binding RelativeSource={RelativeSource Self}}"/>
            <!--<CheckBox Content="Log Selected"
                      IsChecked="{Binding IsSelected}" />-->
        </StackPanel>

    </Grid>
</UserControl>
