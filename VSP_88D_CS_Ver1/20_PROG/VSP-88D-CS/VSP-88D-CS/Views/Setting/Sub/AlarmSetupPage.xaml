<UserControl x:Class="VSP_88D_CS.Views.Setting.Sub.AlarmSetupPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:VSP_88D_CS.Views.Setting"
             xmlns:alarmConfig="clr-namespace:AlarmConfig.Views;assembly=AlarmConfig"
             mc:Ignorable="d"
             d:DesignHeight="700"
             d:DesignWidth="1200">
    <UserControl.Resources>
        <Style BasedOn="{x:Null}"
               TargetType="Button" />
        <Style BasedOn="{x:Null}"
               TargetType="GroupBox" />
    </UserControl.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="30" />
        </Grid.RowDefinitions>
        <alarmConfig:Alarm Grid.Row="0"
                           AlarmList="{Binding AlarmCodes, Mode=TwoWay}"
                           Language="{Binding Language}" />
        <Grid Grid.Row="1" />
    </Grid>
</UserControl>
